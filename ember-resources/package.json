{
  "name": "ember-resources",
  "version": "6.5.0",
  "keywords": [
    "ember-addon"
  ],
  "description": "An implementation of Resources with some helpful utilities",
  "repository": "https://github.com/NullVoxPopuli/ember-resources",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "typesVersions": {
    ">=4.0.0": {
      "*": [
        "declarations/*"
      ]
    }
  },
  "author": "NullVoxPopuli",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "addon-main.cjs",
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "start": "concurrently 'npm:start:*'",
    "build": "concurrently 'npm:build:*'",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:types": "tsc --noEmit",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:js": "eslint . --cache",
    "lint:prettier": "prettier --check '**/*.{js,ts}'",
    "lint:prettier:fix": "prettier --write '**/*.{js,ts}'",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js:fix": "eslint . --fix",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepack": "pnpm run build",
    "start:types": "tsc --emitDeclarationOnly --noEmit false --watch",
    "start:js": "rollup --config --watch --no-watch.clearScreen",
    "build:types": "tsc --emitDeclarationOnly --noEmit false && pnpm fix-bad-declaration-output './declarations/**/*.d.ts'",
    "build:js": "rollup --config"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.2.0",
    "@embroider/macros": "^1.12.3",
    "decorator-transforms": "^1.0.3"
  },
  "peerDependencies": {
    "@glimmer/component": ">= 1.1.2",
    "@glimmer/tracking": ">= 1.1.2",
    "@glint/template": ">= 1.0.0",
    "ember-source": "^3.28.0 || ^4.0.0 || >= 5.0.0"
  },
  "peerDependenciesMeta": {
    "@glimmer/component": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.22.10",
    "@babel/plugin-syntax-decorators": "7.22.10",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/plugin-transform-typescript": "^7.18.4",
    "@babel/preset-typescript": "^7.17.12",
    "@embroider/addon-dev": "^4.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/template": "^1.2.2",
    "@nullvoxpopuli/eslint-configs": "^3.2.0",
    "@tsconfig/ember": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.0.0",
    "ember-source": "5.5.0",
    "ember-template-lint": "5.13.0",
    "eslint": "^8.35.0",
    "expect-type": "^0.16.0",
    "prettier": "^3.1.1",
    "prettier-plugin-ember-template-tag": "^1.0.0",
    "rollup": "4.8.0",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "~5.3.0",
    "fix-bad-declaration-output": "^1.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@glint/core": "^1.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {}
  },
  "packageManager": "pnpm@8.12.1",
  "volta": {
    "extends": "../package.json"
  },
  "type": "module",
  "imports": {
    "#types": "./src/types.ts"
  }
}
